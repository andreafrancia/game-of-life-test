#!/bin/bash
set -o errexit
set -o nounset

program="${1?"
Usage:
    ./test ./your-game-of-life-implementation
"}"

input="\
Generation 1:
4 8
........
....*...
...**...
........
"

diff -u - <($program <<< "$input" ) << EXPECTED_OUTPUT 
Generation 2:
4 8
........
...**...
...**...
........
EXPECTED_OUTPUT
